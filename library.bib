issn = {00983004},
journal = {Computers and Geosciences},
keywords = {Image analysis,Image processing,Operator error,Petrology,Point count,Porosity},
number = {11},
pages = {1850--1859},
publisher = {Elsevier},
title = {{JPOR: An ImageJ macro to quantify total optical porosity from blue-stained thin sections}},
url = {http://dx.doi.org/10.1016/j.cageo.2011.03.002},
volume = {37},
year = {2011}
