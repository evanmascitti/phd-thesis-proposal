@article{Tagar2015,
abstract = {Finite element modeling (FEM) of soil physical behavior can provide information which is difficult or impossible to obtain experimentally. This method has been applied by many researchers to study soil compaction, acting forces on tools, stress distribution in soils and soil failure patterns. The great majority of studies that have investigated soil failure patterns have been limited to in-laboratory soil bins, with few tests being done under field conditions. However, it is difficult to simulate actual soil conditions in a soil bin. This study used FEM for the simulation of the soil failure patterns as linked to consistency limits and sticky point of soil, comparing the simulation results with soil failure patterns observed in the soil bin and in the field. Results showed that FEM is a useful tool to simulate soil failure patterns; however, simulation models correlated better with soil bin than with field test results. The results also showed the presence of a direct relationship between soil failure patterns and the consistency limits of the soil, both in the soil bin and in the field. However, soil bin results were not satisfactorily verified in the field, in particular as the failure patterns were also found to be affected by the roots of the stubbles in the field. It is concluded that FEM can provide accurate simulation of soil failure patterns under soil bin test conditions, but that soil bin results did not satisfactorily represent results from the field.},
author = {Tagar, A. A. and Changying, Ji and Adamowski, Jan and Malard, Julien and Qi, Chen Shi and Qishuo, Ding and Abbasi, N. A.},
doi = {10.1016/j.still.2014.09.006},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tagar et al. - 2015 - Finite element simulation of soil failure patterns under soil bin and field testing conditions.pdf:pdf},
issn = {01671987},
journal = {Soil and Tillage Research},
keywords = {Consistency limits,Finite element method,Paddy soil,Soil physical and mechanical properties,Sticky point},
pages = {157--170},
publisher = {Elsevier B.V.},
title = {{Finite element simulation of soil failure patterns under soil bin and field testing conditions}},
url = {http://dx.doi.org/10.1016/j.still.2014.09.006},
volume = {145},
year = {2015}
}
