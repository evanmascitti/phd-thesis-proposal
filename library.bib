doi = {10.1016/j.proeng.2016.02.034},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Phan, Hsiao, Nguyen - 2016 - Effects of fines contents on engineering properties of sand-fines mixtures.pdf:pdf},
