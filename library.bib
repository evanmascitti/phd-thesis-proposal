journal = {New Zealand Journal of Agricultural Research},
keywords = {apical dominance,carbohydrate,gibberellic-acid,gibberellin,inhibition,leaf appearance,light-intensity,low-temperature,paclobutrazol,perennial ryegrass,phyllochron,responses,rice,site filling,tillering,vs. root-growth,wheat},
