@article{Altuhafi2016,
abstract = {The effect of particle shape on the compressional properties of various batches of starch (Starch 1500; I), sodium chloride (II), lactose (III), and calcium phosphate dibasic dihydrate (Emcompress; IV) was investigated and measurements of tensile strength, compressibility, elastic recovery, yield values and fragmentation propensities were made. For materials which consolidate by plastic deformation (I and II), there is a large increase in compressibility, a large decrease in yield values and a small decrease in elastic recovery in going from regular to irregular particles. This explains the observed increase in tensile strength which is due to the increased area of contact between particles as they deform. For materials which consolidate by fragmentation (III and IV), the shape of the particles has practically no effect on the above properties. However, the irregular particles are found to fracture more than the regular ones as shown by the values of Pfb and fragmentation propensity. (14 refs.) (Modified author abstract)},
author = {Altuhafi, Fatin N. and Coop, Matthew R. and Georgiannou, Vasiliki N.},
doi = {10.1061/(ASCE)GT.1943-5606.0001569.},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Altuhafi, Coop, Georgiannou - 2016 - Effect of particle shape on the mechanical properties of natural sands.pdf:pdf},
issn = {10900241},
journal = {Journal of Geotechnical and Geoenvironmental Engineering},
keywords = {angularity,critical state,dynamic imaging analysis,particle shape,sand},
number = {07},
