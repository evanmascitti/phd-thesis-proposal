journal = {Geoderma},
keywords = {Compaction,Friability,Hard-setting,Tillage,Tilth,Water retention curve},
number = {3-4},
pages = {215--225},
title = {{Soil physical quality: Part II. Friability, tillage, tilth and hard-setting}},
volume = {120},
year = {2004}
}
