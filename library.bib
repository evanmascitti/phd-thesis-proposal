journal = {Journal of Testing and Evaluation},
keywords = {Kentucky bluegrass (Poa pratensis L.),athletic footwear,creeping red,creeping red fescue (Festuca rubra L.),cutting height,fescue,festuca arundinacea schreb,festuca rubra l,kentucky bluegrass,loliumperenne l,perennial ryegrass,perennial ryegrass (Lolium perenne L.),playing fields,poapratensis l,shoes,sports fields,sports injuries,tall fescue,tall fescue (Festuca arun,traction,turfgrass},
