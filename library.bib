doi = {10.3109/17435390.2010.528846},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Taurozzi, Hackley, Wiesner - 2011 - Ultrasonic dispersion of nanoparticles for environmental, health and safety assessment issues and re.pdf:pdf},
issn = {17435390},
journal = {Nanotoxicology},
keywords = {Dispersion,Environmental assessment,Nanomaterial,Nanoparticle,Sonication,Suspension,Toxicology,Ultrasonics},
number = {4},
pages = {711--729},
title = {{Ultrasonic dispersion of nanoparticles for environmental, health and safety assessment issues and recommendations}},
volume = {5},
year = {2011}
}
