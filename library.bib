doi = {10.1016/j.clay.2007.06.007},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/de Oliveira Modesto, Bernardin - 2008 - Determination of clay plasticity Indentation method versus Pfefferkorn method.pdf:pdf},
isbn = {0169-1317},
issn = {01691317},
journal = {Applied Clay Science},
