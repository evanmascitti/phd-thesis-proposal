doi = {10.1007/bf02903531},
file = {:C$\backslash$:/Users/ecm200/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Cross - 2010 - Measurements of the horizontal and vertical speeds of tennis courts.pdf:pdf},
issn = {1369-7072},
journal = {Sports Engineering},
keywords = {bounce,coeffcient of friction,coeffcient of restitution,court speed},
number = {2},
pages = {95--111},
title = {{Measurements of the horizontal and vertical speeds of tennis courts}},
volume = {6},
